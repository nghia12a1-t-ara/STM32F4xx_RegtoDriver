#include <stdint.h>
#include "UART_REG.h"

int main(void)
{
	uint8_t pRxBuffer[12][8] = {};

	USART_Init(UART_MODE_SLAVE);

    /* Loop forever */
	for(;;)
	{
		for (int i = 0; i < 11; i++)
		{
			USART_ReceiveData(&pRxBuffer[i], 8);
		}
	}
}





