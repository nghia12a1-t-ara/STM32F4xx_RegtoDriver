#include <stdint.h>
#include "UART_REG.h"


int main(void)
{
	uint8_t pRxBuffer[8];

	USART_Init(UART_MODE_SLAVE|(UART_MODE_MASTER));

	UART_SendData("ABCD", 5);

	UART_SendData("123456789", 10);

    /* Loop forever */
	for(;;)
	{
		UART_ReceiveData(&pRxBuffer, 8);
	}
}





